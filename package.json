{
  "name": "atlassian-mcp-client",
  "version": "1.0.0",
  "description": "Model Context Protocol client for Atlassian's Remote MCP Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "ai": "tsx src/cli/ai-cli.ts",
    "ai:chat": "tsx src/cli/ai-cli.ts chat",
    "test:gemini": "tsx src/test/test-gemini.ts",
    "demo": "tsx src/examples/interactive-cli.ts",
    "demo:jira": "tsx src/examples/jira-demo.ts",
    "demo:confluence": "tsx src/examples/confluence-demo.ts",
    "demo:interactive": "tsx src/examples/interactive-cli.ts",
    "demo:mcp-remote": "tsx src/demo/mcp-remote-demo.ts",
    "demo:working": "tsx src/demo/working-demo.ts",
    "start:proxy": "tsx src/demo/start-proxy.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "atlassian",
    "jira",
    "confluence",
    "oauth",
    "sse"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "inquirer": "^9.2.12",
    "open": "^10.0.3",
    "ora": "^8.2.0",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
