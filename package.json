{
  "name": "atlassian-mcp-client",
  "version": "1.0.0",
  "description": "Simple AI-powered Model Context Protocol client for Atlassian's Remote MCP Server",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "atlassian-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "ai": "tsx run-ai-direct.ts",
    "enhanced": "tsx run-enhanced-direct.ts",
    "simple": "tsx run-simple-enhanced.ts",
    "test:bedrock": "tsx src/test/test-bedrock.ts",
    "test:enhanced": "tsx src/test/test-enhanced-ai.ts",
    "test:multi-server": "tsx src/demo/test-multi-server.ts",
    "test:knowledge-hub": "tsx src/test/test-knowledge-hub.ts",
    "ui": "tsx src/ui/server.ts",
    "ui:test": "tsx src/ui/test-ui.ts",
    "test:path": "node test-path.js",
    "connect": "tsx src/index.ts connect",
    "tools": "tsx src/index.ts tools",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "atlassian",
    "mcp",
    "model-context-protocol",
    "jira",
    "confluence",
    "ai",
    "aws-bedrock",
    "claude",
    "typescript",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.858.0",
    "@aws-sdk/client-bedrock-runtime": "^3.540.0",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "tsx": "^4.6.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}